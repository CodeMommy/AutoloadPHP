{
    "name": "codemommy/autoloadphp",
    "version": "0.0.6",
    "type": "library",
    "description": "Automatic load namespaces, classes and files",
    "keywords": [
        "CodeMommy",
        "AutoloadPHP",
        "Autoload",
        "PHP"
    ],
    "license": "Apache 2.0",
    "homepage": "http://www.codemommy.com",
    "support": {
        "issues": "https://github.com/CodeMommy/AutoloadPHP/issues",
        "source": "https://github.com/CodeMommy/AutoloadPHP"
    },
    "authors": [
        {
            "name": "Candison November",
            "email": "kandisheng@163.com",
            "homepage": "http://www.kandisheng.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CodeMommy\\AutoloadPHP\\": "source/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeMommy\\AutoloadPHP\\Script\\": "script/"
        }
    },
    "scripts": {
        "clean": [
            "CodeMommy\\AutoloadPHP\\Script\\Command::cleanReport"
        ],
        "test": [
            "CodeMommy\\AutoloadPHP\\Script\\Command::cleanReport",
            "\"vendor/bin/phpunit\" -v",
            "start .report/index.html"
        ],
        "update-project": [
            "git pull",
            "composer self-update",
            "composer update"
        ],
        "update-version": "CodeMommy\\AutoloadPHP\\Script\\Command::updateVersion"
    },
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "CodeMommy/TaskPHP": "*",
        "phpunit/phpunit": "*"
    }
}